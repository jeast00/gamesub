<h2>Log In</h2>

<%= form_tag sessions_path do %>
    <%= label_tag :email %>
    <%= text_field_tag :email %><br><br>

    <%= label_tag :password %>
    <%= password_field_tag :password %><br><br>

    <%= submit_tag "log in" %>
<% end %>

<p>Don't have an account? <%= link_to "Sign up", "/signup" %> here!</p>

<p><%= link_to "Log in with GitHub", "/auth/github" %></p>
