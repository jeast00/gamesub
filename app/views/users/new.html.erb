<h2>Sign Up Here!</h2>

<%= form_for @user do |f| %>
    <%= f.label :user_name %>
    <%= f.text_field :user_name %>

    <%= f.label :first_name %>
    <%= f.text_field :first_name %>

    <%= f.label :last_name %>
    <%= f.text_field :last_name %>

    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit "Sign Up" %>    
<% end %>